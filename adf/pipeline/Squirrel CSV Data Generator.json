{
	"name": "DailyCSVGenerator_SquirrelDataset",
	"properties": {
		"activities": [
			{
				"name": "Run Generate Squirrel Data",
				"type": "SynapseNotebook",
				"dependsOn": [
					{
						"activity": "Set variable run_date",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": {
							"value": "GenerateDailyData_SquirrelDataSet",
							"type": "Expression"
						},
						"type": "NotebookReference"
					},
					"parameters": {
						"param_run_date": {
							"value": {
								"value": "@if(\n    empty(string(pipeline().parameters.p_run_date)),\n    formatDateTime(utcNow(), 'yyyyMMdd'),\n    formatDateTime(\n        concat(\n            substring(pipeline().parameters.p_run_date, 0, 4),'-',\n            substring(pipeline().parameters.p_run_date, 4, 2),'-',\n            substring(pipeline().parameters.p_run_date, 6, 2)\n        ),\n        'yyyy-MM-dd'\n    )\n)\n",
								"type": "Expression"
							},
							"type": "string"
						}
					},
					"snapshot": true,
					"sparkPool": {
						"referenceName": {
							"value": "splendtoenddemo",
							"type": "Expression"
						},
						"type": "BigDataPoolReference"
					},
					"executorSize": "Small",
					"conf": {
						"spark.dynamicAllocation.enabled": null,
						"spark.dynamicAllocation.minExecutors": null,
						"spark.dynamicAllocation.maxExecutors": null
					},
					"driverSize": "Small",
					"numExecutors": null
				},
				"linkedServiceName": {
					"referenceName": "LS_ASA_synwendtoenddemo",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Set variable run_date",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "dummy_var",
					"value": {
						"value": "@if(\n    empty(string(pipeline().parameters.p_run_date)),\n    formatDateTime(utcNow(), 'yyyyMMdd'),\n    formatDateTime(\n        concat(\n            substring(pipeline().parameters.p_run_date, 0, 4),'-',\n            substring(pipeline().parameters.p_run_date, 4, 2),'-',\n            substring(pipeline().parameters.p_run_date, 6, 2)\n        ),\n        'yyyy-MM-dd'\n    )\n)\n",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"p_run_date": {
				"type": "string"
			}
		},
		"variables": {
			"dummy_var": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Utilities"
		},
		"annotations": []
	}
}